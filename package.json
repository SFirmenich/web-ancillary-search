{
  "name": "andes",
  "repository": "https://bitbucket.org/gabybosetti/web-ancillary-search",
  "version": "1.0.0",
  "description": "ANDES",
  "scripts": {
    "web-ext-prepare": "pushd ./src && npm install && popd",
    "start:chrome": "npm-run-all web-ext-prepare && chrome --temp-profile --password-store=basic --load-extension=$(pwd)/src",
    "start:chromium": "npm-run-all web-ext-prepare && chromium --temp-profile --password-store=basic --load-extension=$(pwd)/src",
    "start:firefox": "web-ext run -s src",
    "start:nightly": "web-ext run -s src --firefox=nightly",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LIFIA",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "web-ext": "^2.7.0"
  }
}
