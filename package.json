{
  "name": "andes",
  "repository": "https://bitbucket.org/gabybosetti/web-ancillary-search",
  "version": "1.0.0",
  "description": "ANDES",
  "scripts": {
    "web-ext-prepare": "pushd ./src && npm install && popd",
    "serve": "npm-run-all web-ext-prepare && grunt && grunt watch&",
    "start:chrome": "npm-run-all serve && chrome --load-extension=$(pwd)/build/web-ancillary-search",
    "start:chromium": "npm-run-all serve && chromium --load-extension=$(pwd)/build/web-ancillary-search",
    "start:firefox": "npm-run-all serve && web-ext run -s build/web-ancillary-search",
    "start:nightly": "npm-run-all serve && web-ext run -s build/web-ancillary-search --firefox=nightly",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LIFIA",
  "license": "MIT",
  "devDependencies": {
    "grunt": "^1.0.3",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-watch": "^1.1.0",
    "npm-run-all": "^4.1.3",
    "web-ext": "^2.7.0"
  },
  "dependencies": {}
}
